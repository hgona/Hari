package Training;

import java.util.Scanner;

public class Calculator1 {

	public void PrintTypeAdd() {
		Scanner Addition = new Scanner(System.in);
		System.out.println("Enter your Range: ");
		int Ran = Addition.nextInt();
		int result = 0;
		int c = 0;
		for (int a = 1; a <= Ran; a++) {
			System.out.println("Enter your  value " + a);
			c = Addition.nextInt();
		}
		for (int i = 1; i <= Ran; i++) {
			result = c + result;
		}
		System.out.println(result);
	}

	public static void main(String[] args) {
		Calculator1 add = new Calculator1();
		add.PrintTypeAdd();
	}

}
