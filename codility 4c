class Solution {
    public int solution(int[] A) {
        // write your code in Java SE 7
        int[] count = new int[A.length];
            System.out.println(count[2]);
        for (int i = 0; i < A.length; i++) {

            int x = A[i]-1;

            if (x >= A.length) return 0;
             System.out.println(x);
            int check = count[x];
           System.out.println(check);
            if (check > 0) return 0;

            count[x] = 1;
        }

        return 1;
    }
}