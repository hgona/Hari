public class Fibonacci {
	public static void main(String[]args){
	    int Fmax=20;
		int fn=0;
		int f1=0;
		int f2=1;
		int sum=0;
		double avg ;
		for(int n=1;n<=Fmax;n++) {
			f1=f2;
			f2=fn;
			fn=f1+f2; 
			System.out.print(" "+fn);
			sum=sum+fn;
			}                                                     
	    System.out.println("");
	    avg=(double)sum/Fmax;
	    System.out.println(sum);
		System.out.println(Fmax);
		System.out.println( " The Average is "+ avg);
		 }
}
